{
  "name": "npm-global-audit",
  "version": "1.2.0",
  "description": "CLI to perform an audit of globally installed node modules",
  "license": "MIT",
  "author": "Andrew Brey <34140052+andrewbrey@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/LaserTacoCat/npm-global-audit"
  },
  "bugs": "https://github.com/LaserTacoCat/npm-global-audit/issues",
  "scripts": {
    "dev": "./bin/run",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib npm-global-audit-*.tgz && tsc -b && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "npm-global-audit": "./bin/run"
  },
  "oclif": {
    "bin": "npm-global-audit"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "execa": "5.1.1",
    "fs-extra": "10.0.0",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@types/fs-extra": "9.0.11",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "audit",
    "cli",
    "global",
    "npm",
    "security"
  ]
}
